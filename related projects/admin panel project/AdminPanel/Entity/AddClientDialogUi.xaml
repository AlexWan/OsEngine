<Window x:Class="AdminPanel.Entity.AddClientDialogUi"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="New client" Height="206" Width="437" Topmost="True" Style="{StaticResource WindowStyleNoResize}" ResizeMode="NoResize" WindowStartupLocation="CenterScreen" Icon="/Images/OsLogo.ico">
    <Grid>
        <Rectangle Margin="5,5,5,40"/>
        <TextBlock Name="TbText" Margin="20,46,20,0" VerticalAlignment="Top"
                   FontWeight="Bold" FontSize="14"
                   Height="65" TextWrapping="Wrap"
                   Foreground="Red"></TextBlock>
        <TextBox Name="ClientName" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="20,18,0,0" Width="262"></TextBox>
        <Button Name="ButtonAccept" Content="Accept" HorizontalAlignment="Right" Margin="0,0,5,10" VerticalAlignment="Bottom" Width="132" Click="ButtonAccept_Click" />
    </Grid>
</Window>
