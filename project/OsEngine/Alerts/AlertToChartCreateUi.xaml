        <Label Name="LabelReactionType" Content="Reaction" HorizontalAlignment="Left" Margin="38,101,0,0" VerticalAlignment="Top" />
        <ComboBox Name="ComboBoxSignalType" HorizontalAlignment="Left" Margin="124,101,0,0" VerticalAlignment="Top" Width="103"/>
        <Slider Name="Slider" Margin="0,22,26,18" Maximum="200" SmallChange="1" Orientation="Vertical" Value="100" HorizontalAlignment="Right" Width="22"/>
        <TextBox Name="TextBoxVolumeReaction" HorizontalAlignment="Right" Height="23" Margin="0,97,97,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="96"/>
        <Label Name="LabelVolume" Content="Volume" HorizontalAlignment="Right" Margin="0,97,195,0" VerticalAlignment="Top" />
        <ComboBox Name="ComboBoxOrderType" HorizontalAlignment="Left" Margin="124,129,0,0" VerticalAlignment="Top" Width="103" />
        <Label Name="LabelOrderType" Content="Order type" HorizontalAlignment="Left" Margin="38,129,0,0" VerticalAlignment="Top" />
        <TextBox Name="TextBoxSlippage" HorizontalAlignment="Right" Height="23" Margin="0,125,97,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="96" />
        <Label Name="LabelSlippage" Content="Slippage" HorizontalAlignment="Right" Margin="0,125,195,0" VerticalAlignment="Top" />
        <TextBox Name="TextBoxClosePosition" HorizontalAlignment="Right" Height="23" TextWrapping="Wrap" VerticalAlignment="Top" Width="96" Margin="0,181,162,0" />
        <Label Name="LabelNumClosedPos" Content="Number of position to be closed" HorizontalAlignment="Right" Margin="0,181,258,0" VerticalAlignment="Top"/>
        <Label Name="LabelTrade" Content="Trading" HorizontalAlignment="Left" Margin="29,66,0,0" VerticalAlignment="Top" FontSize="18" FontWeight="Bold"/>
        <Label Name="LabelFireworks" Content="Fireworks" HorizontalAlignment="Left" Margin="29,228,0,0" VerticalAlignment="Top" FontSize="18" FontWeight="Bold"/>
        <ComboBox Name="ComboBoxMusicType" HorizontalAlignment="Left" Margin="166,261,0,0" VerticalAlignment="Top" Width="79" ></ComboBox>
        <Rectangle Fill="#FCEE21" Height="179" Stroke="Black" StrokeThickness="0" VerticalAlignment="Bottom" HorizontalAlignment="Left" Width="20" RenderTransformOrigin="0.5,0.5" Margin="1,0,0,0">
            <Rectangle.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform AngleY="-44"/>
                    <RotateTransform/>
                    <TranslateTransform Y="-12.012"/>
                </TransformGroup>
            </Rectangle.RenderTransform>
        </Rectangle>
        <Label Name="LabelOsa" Content="O-S-A.NET" Foreground="Black" HorizontalAlignment="Left" Margin="-28,0,0,89" VerticalAlignment="Bottom" FontWeight="Bold" RenderTransformOrigin="0.5,0.5" FontSize="14">
            <Label.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform/>
                    <RotateTransform Angle="-90"/>
                    <TranslateTransform/>
                </TransformGroup>
            </Label.RenderTransform>
        </Label>
        <Label Name="LabelSlippageType" Content="Slippage type" HorizontalAlignment="Right" Margin="0,153,194,0" VerticalAlignment="Top" />
        <ComboBox Name="ComboBoxSlippageType" Margin="0,153,97,0" VerticalAlignment="Top" HorizontalAlignment="Right" Width="96" />
    </Grid>
</Window>
