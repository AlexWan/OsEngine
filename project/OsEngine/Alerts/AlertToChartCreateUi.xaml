<Window x:Class="OsEngine.Alerts.AlertToChartCreateUi"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:system="clr-namespace:System;assembly=mscorlib"
        Title="Alert Settings" Height="475" Width="568" ResizeMode="NoResize" Style="{StaticResource WindowStyleNoResize}" Topmost="True" WindowStartupLocation="CenterScreen" Icon="/OsEngine;component/Images/OsLogo.ico">
    <Grid>
        <Rectangle   Margin="5,219,71,39"/>
        <Rectangle   Margin="5,61,71,0" Height="153" VerticalAlignment="Top"/>
        <Rectangle   Margin="5,5,5,0" Height="51" VerticalAlignment="Top"/>
        <Rectangle   Margin="0,10,5,10" HorizontalAlignment="Right" Width="61"/>
        <ComboBox Name="ComboBoxType" HorizontalAlignment="Left" Margin="194,26,0,0" VerticalAlignment="Top" Width="185"/>
        <Button Name="ButtonSendFirst" Content="Send" HorizontalAlignment="Left" Margin="394,26,0,0" VerticalAlignment="Top" Width="65" Click="ButtonSendFirst_Click"/>
        <Button Name="ButtonSave" Content="Accept" Margin="0,0,71,12" VerticalAlignment="Bottom" Click="ButtonSave_Click" HorizontalAlignment="Right" Width="132"/>
        <CheckBox Name="CheckBoxMusicAlert" Content="Sound" HorizontalAlignment="Left" Margin="44,261,0,0" VerticalAlignment="Top"/>
        <CheckBox Name="CheckBoxOnOff" Content="On/Off"  HorizontalAlignment="Left" Margin="29,22,0,0" VerticalAlignment="Top"  FontSize="18" FontWeight="Bold"/>
        <Label Name="LabelToopTipText" Content="ToolTip Text" HorizontalAlignment="Left" Margin="45,320,0,0" VerticalAlignment="Top" />
        <TextBox Name="TextBoxLabelAlert" HorizontalAlignment="Left" Height="45" Margin="38,342,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="207" />
        <Label Name="LabelLineColor" Content="Line Color" HorizontalAlignment="Right" Margin="0,289,195,0" VerticalAlignment="Top"/>
