        <CheckBox Name="CheckBoxWindow" Content="Popup window" Margin="261,320,0,0" VerticalAlignment="Top" HorizontalAlignment="Left" Width="132"/>
        <Label Name="LabelLineWidth" Content="Line width" HorizontalAlignment="Left" Margin="45,292,0,0" VerticalAlignment="Top" />
        <ComboBox Name="ComboBoxFatLine" HorizontalAlignment="Left" Margin="166,292,0,0" VerticalAlignment="Top" Width="79">
