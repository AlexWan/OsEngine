<Window x:Class="OsEngine.Charts.CandleChart.Indicators.IndicarotCreateUi"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Добавить индикатор" Height="360" Width="408" ResizeMode="NoResize" Style="{StaticResource WindowStyleNoResize}" WindowStartupLocation="CenterScreen" Icon="/Images/OsLogo.ico" Topmost="True">
    <Grid>
        <Rectangle Margin="5,5,5,37">
        </Rectangle>
        <WindowsFormsHost Name="HostArea" Margin="216,10,10,50"/>
        <Button x:Name ="ButtonAccept" Content="Accept" HorizontalAlignment="Right" Margin="0,0,10,10" VerticalAlignment="Bottom" Width="150" Click="ButtonAccept_Click" />
        <TabControl Name="TabControlIndicatorsNames" HorizontalAlignment="Left" Height="276" Margin="10,0,0,0" VerticalAlignment="Top" Width="201">
            <TabItem Name="ItemIncluded" Header="Include">
                <Grid Background="#FFE5E5E5">
                    <WindowsFormsHost x:Name ="HostNames"/>
                </Grid>
            </TabItem>
            <TabItem Name="TabItemScript" Header="Script">
                <Grid Background="#FFE5E5E5" Margin="0,0,0,0">
                    <WindowsFormsHost Name="HostNamesScript" Margin="0,0,0,0"/>
                </Grid>
            </TabItem>
        </TabControl>
    </Grid>
</Window>