        <TextBox x:Name ="TextBoxShiftUp" HorizontalAlignment="Left" Height="23" Margin="155,70,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Label Name="LabelIndicatorShift1" Content="Shift" HorizontalAlignment="Right" Margin="0,70,478,0" VerticalAlignment="Top"/>
        <Label Name="LabelIndicatorAlligator3" Content="Alligator's Teeth. Middle" HorizontalAlignment="Right" Margin="0,137,345,0" VerticalAlignment="Top" FontWeight="Bold"/>
        <TextBox x:Name ="TextBoxLenghtBase" HorizontalAlignment="Left" Height="23" Margin="155,177,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Label Name="LabelIndicatorPeriod3" Content="Period" Margin="0,177,478,0" VerticalAlignment="Top" HorizontalAlignment="Right" Width="45" />
        <TextBox x:Name ="TextBoxShiftBase" HorizontalAlignment="Left" Height="23" Margin="155,205,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
