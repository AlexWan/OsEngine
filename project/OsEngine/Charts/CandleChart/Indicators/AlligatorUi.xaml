        <Label Name="LabelIndicatorPeriod2" Content="Period" HorizontalAlignment="Right" Margin="0,44,182,0" VerticalAlignment="Top" />
        <TextBox x:Name ="TextBoxShiftDown" Height="23" Margin="420,70,23,0" TextWrapping="Wrap" VerticalAlignment="Top"/>
        <Label Name="LabelIndicatorShift3" Content="Shift" HorizontalAlignment="Right" Margin="0,70,182,0" VerticalAlignment="Top"/>
        <ComboBox x:Name="ComboBoxMovingType" Margin="420,177,23,0" VerticalAlignment="Top"/>
        <Label Name="LabelIndicatorMethod" Content="Calculation method" HorizontalAlignment="Right" Margin="0,177,182,0" VerticalAlignment="Top"/>

