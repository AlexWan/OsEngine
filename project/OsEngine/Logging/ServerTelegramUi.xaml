<Window x:Class="OsEngine.Logging.ServerTelegramDeliveryUi"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:system="clr-namespace:System;assembly=mscorlib"
        Title="Telegram server" Height="193" Width="331" Style="{StaticResource WindowStyleNoResize}" ResizeMode="NoResize" WindowStartupLocation="CenterScreen" Icon="/Images/OsLogo.ico" Topmost="True">
    <Grid>
        <Button Name="ButtonAccept" Content="Accept" Margin="0,0,5,10" Click="buttonAccept_Click" VerticalAlignment="Bottom" HorizontalAlignment="Right" Width="101"/>
        <Rectangle Margin="5,5,5,37"/>
        <TextBox Name="TextBoxMyBotToken" Height="23" Margin="115,20,19,0" TextWrapping="Wrap"  VerticalAlignment="Top"/>
        <TextBox Name="TextBoxChatId" Height="23" Margin="115,48,19,0" TextWrapping="Wrap"  VerticalAlignment="Top"/>
        <Label Name="Label23" Content="Bot Token" HorizontalAlignment="Left" Margin="20,20,0,0" VerticalAlignment="Top" Width="81"/>
        <Label Name="Label24" Content="Chat  ID" HorizontalAlignment="Left" Margin="20,48,0,0" VerticalAlignment="Top" Width="67"/>
        <CheckBox Name="CheckBoxTelegramProcessingCommand" Content="Processing Command from Telegram" HorizontalAlignment="Left" Margin="20,80,0,0" VerticalAlignment="Top"/>
        <Label Name="Label25" Content="Check input Chat Id. Only numbers are acceptable." HorizontalAlignment="Center" Margin="0,105,0,0" VerticalAlignment="Top" Width="291" Height="20" FontSize="12" Foreground="#FFFF1D12" HorizontalContentAlignment="Center" IsEnabled="False"/>

    </Grid>
</Window>
