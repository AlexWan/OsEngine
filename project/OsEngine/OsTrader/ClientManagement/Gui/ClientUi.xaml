<Window x:Class="OsEngine.OsTrader.ClientManagement.Gui.ClientUi"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:OsEngine.OsTrader.ClientManagement.Gui"
        mc:Ignorable="d"
        Title="Client" Height="574" Width="594" MinWidth="650" MinHeight="400" WindowStartupLocation="CenterScreen" Style="{StaticResource WindowStyleCanResize}" Icon="/Images/OsLogo.ico">
    <Grid>
        <TabControl Foreground="Black" Margin="0,66,0,0">
            <TabItem Name="TabItem1" Width="100" Header=" Connectors ">
                <Grid>
                    <WindowsFormsHost Name="HostClientConnectors" Foreground="Black" Margin="0,0,0,0"/>
                </Grid>
            </TabItem>
            <TabItem Name="TabItem2" Width="100" Header=" Robots ">
                <Grid>
                    <WindowsFormsHost Name="HostClientRobots" Foreground="Black" Margin="0,0,0,0"/>
                </Grid>
            </TabItem>
            <TabItem Name="TabItem3" Width="100" Header=" Log ">
                <Grid>
                    <WindowsFormsHost Name="HostClientLog" Foreground="Black" Margin="0,0,0,0"/>
                </Grid>
            </TabItem>
        </TabControl>
        <Label Name="LabelName" Content="Name" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top"/>
        <TextBox Name="TextBoxClientName" HorizontalAlignment="Left" Margin="99,10,0,0" TextWrapping="Wrap"  VerticalAlignment="Top" Width="120"/>
        <Label Name="LabelRegime" Content="Regime" HorizontalAlignment="Left" Margin="10,38,0,0" VerticalAlignment="Top"/>
        <ComboBox Name="ComboBoxRegime" HorizontalAlignment="Left" Margin="98,38,0,0" VerticalAlignment="Top" Width="122"/>
        <Label Name="LabelUid" Content="Uid" HorizontalAlignment="Left" Margin="254,10,0,0" VerticalAlignment="Top"/>
        <TextBox Name="TextBoxUid" Margin="343,10,10,0" TextWrapping="Wrap"  VerticalAlignment="Top"/>


    </Grid>
</Window>
