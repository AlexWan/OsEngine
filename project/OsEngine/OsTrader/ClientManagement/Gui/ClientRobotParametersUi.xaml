<Window x:Class="OsEngine.OsTrader.ClientManagement.Gui.ClientRobotParametersUi"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:OsEngine.OsTrader.ClientManagement.Gui"
        mc:Ignorable="d"
        Title="Robot parameters settings" Height="416" Width="341" MinWidth="490" MinHeight="200" Style="{StaticResource WindowStyleCanResize}" WindowStartupLocation="CenterScreen" Icon="/Images/OsLogo.ico">
    <Grid Name="GridPrime">
        <Grid.RowDefinitions>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <Rectangle Margin="0,0,0,0"/>
        <TabControl Name="TabControlSettings" Margin="5,33,5,5">

            <TabItem Name="TabItemParameters" Header=" Parameters ">
                <Grid >
                    <WindowsFormsHost Name="HostSettings"  Margin="0,0,0,0"/>
                </Grid>
            </TabItem>

        </TabControl>
        <ComboBox Name="ComboBoxRobotType" HorizontalAlignment="Left" Margin="188,5,0,0" VerticalAlignment="Top" Width="176"/>
        <Label Name="LabelRobotType" Content="Robot type" HorizontalAlignment="Left" Margin="83,5,0,0" VerticalAlignment="Top"/>
    </Grid>
</Window>