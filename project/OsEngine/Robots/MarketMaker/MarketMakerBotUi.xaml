<Window x:Class="OsEngine.Robots.MarketMaker.MarketMakerBotUi"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Strategy settings" Height="292" Width="330" Icon="/Images/OsLogo.ico" Style="{StaticResource WindowStyleNoResize}" ResizeMode="NoResize" WindowStartupLocation="CenterScreen" Topmost="True">
    <Grid>
        <Rectangle Margin="5,5,5,37"/>
        <Label Name="LabelVolume" Content="Volume" HorizontalAlignment="Left" Margin="23,116,0,0" VerticalAlignment="Top" />
        <TextBox Name="TextBoxVolumeOne" Height="23" Margin="173,116,19,0" TextWrapping="Wrap"  VerticalAlignment="Top"/>
        <Button Name="ButtonAccept" Content="Accept" HorizontalAlignment="Right" Margin="0,0,5,7" VerticalAlignment="Bottom" Width="120" Click="Button_Click"/>
        <ComboBox Name="ComboBoxRegime" Margin="173,21,19,0" VerticalAlignment="Top"/>
        <Label Name="LabelRegime" Content="Regime" HorizontalAlignment="Left" Margin="23,21,0,0" VerticalAlignment="Top" />
        <Label Name="LabelBetweenLines" Content="% between the lines" HorizontalAlignment="Left" Margin="23,148,0,0" VerticalAlignment="Top" />
        <TextBox Name="TextBoxSpreadBeetwenLine" Height="23" Margin="173,148,19,0" TextWrapping="Wrap"  VerticalAlignment="Top"/>
        <CheckBox Name="CheckBoxNeedToPaint" Content="Is painting" HorizontalAlignment="Left" Margin="173,176,0,0" VerticalAlignment="Top" />
        <Button Name="ButtonAbout" Content="???" HorizontalAlignment="Left" Margin="5,0,0,7" VerticalAlignment="Bottom" Width="43" Click="ButtonAbout_Click"/>
        <TextBox Name="TextBoxAssetInPortfolio" IsReadOnly="True" Height="23" Margin="173,84,19,0" TextWrapping="Wrap"  VerticalAlignment="Top"/>
        <ComboBox Name="ComboBoxVolumeType" Margin="173,53,19,0" VerticalAlignment="Top"/>
        <Label Name="LabelAssetInPortfolio" Content="Asset in portfolio" HorizontalAlignment="Left" Margin="23,84,0,0" VerticalAlignment="Top" />
        <Label Name="LabelVolumeType" Content="Volume Type" HorizontalAlignment="Left" Margin="23,53,0,0" VerticalAlignment="Top" />
    </Grid>
</Window>