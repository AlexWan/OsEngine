<Window x:Class="OsEngine.Robots.MyBots.WindowMyRobot"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:OsEngine.Robots.MyBots.MyRobot"

        mc:Ignorable="d"
        Title="WindowMyRobot" Height="450" Width="800">
    <Window.Resources>
        <Style TargetType="TextBlock">
            <Setter Property="FontSize" Value="22"/>
            <Setter Property="HorizontalAlignment" Value="Center"/>
            <Setter Property="VerticalAlignment" Value="Center"/>

        </Style>

    </Window.Resources>

    <StackPanel Orientation="Vertical">

        <TextBlock Text="Привет я робот"/>
        <TextBlock x:Name="LotTextBlock"/>
        <TextBlock x:Name="StopTextBlock"/>
        <TextBlock x:Name="TakeTextBlock"/>



    </StackPanel>
</Window>
