<Window x:Class="OsEngine.Robots.Trend.PriceChannelTradeUi"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
          Title="Settings" Height="252" Width="315.496" Icon="/Images/OsLogo.ico" Style="{StaticResource WindowStyleNoResize}" ResizeMode="NoResize" WindowStartupLocation="CenterScreen" Topmost="True">
    <Grid Margin="0,0,0,1">
        <Rectangle Margin="5,5,5,37"/>
        <Label Name="LabelVolume" Content="Volume" HorizontalAlignment="Left" Margin="23,112,0,0" VerticalAlignment="Top" />
        <TextBox Name="TextBoxVolumeOne"  HorizontalAlignment="Left" Height="23" Margin="173,112,0,0" TextWrapping="Wrap"  VerticalAlignment="Top" Width="120"/>
        <Label Name="LabelSlippage" Content="Slippage" HorizontalAlignment="Left" Margin="23,139,0,0" VerticalAlignment="Top" />
        <TextBox Name="TextBoxSlippage" HorizontalAlignment="Left" Height="23" Margin="173,139,0,0" TextWrapping="Wrap"  VerticalAlignment="Top" Width="120"/>
        <Button Name="ButtonAccept" Content="Accept" Margin="0,0,10,10" Click="Button_Click" Height="22" VerticalAlignment="Bottom" HorizontalAlignment="Right" Width="120"/>
        <ComboBox Name="ComboBoxRegime" HorizontalAlignment="Left" Margin="173,24,0,0" VerticalAlignment="Top" Width="120"/>
        <Label Name="LabelRegime" Content="Regime" HorizontalAlignment="Left" Margin="23,24,0,0" VerticalAlignment="Top" />
        <Label Name="LabelAssetInPortfolio" Content="Asset in portfolio" HorizontalAlignment="Left" Margin="23,84,0,0" VerticalAlignment="Top" />
        <Label Name="LabelVolumeType" Content="Volume Type" HorizontalAlignment="Left" Margin="23,53,0,0" VerticalAlignment="Top" />
        <TextBox Name="TextBoxAssetInPortfolio" IsReadOnly="True" Height="23" Margin="173,84,20,0" TextWrapping="Wrap"  VerticalAlignment="Top"/>
        <ComboBox Name="ComboBoxVolumeType" Margin="173,53,20,0" VerticalAlignment="Top"/>
    </Grid>
</Window>
