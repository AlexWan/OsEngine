<Window x:Class="OsEngine.Market.Servers.QuikLua.QuikLuaServerUi"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Подключение к Quik" Height="295" Width="662" MinWidth="690" MinHeight="200" Style="{StaticResource WindowStyleCanResize}" Topmost="True" WindowStartupLocation="CenterScreen" Icon="/Images/OsLogo.ico">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="125*"/>
            <ColumnDefinition Width="101*"/>
        </Grid.ColumnDefinitions>
        <Rectangle Margin="5,5,5,42" Grid.ColumnSpan="2"/>
        <Button Name="ButtonConnect" Content="Подключить" Margin="0,0,144,10" VerticalAlignment="Bottom" Click="ButtonConnect_Click" HorizontalAlignment="Right" Width="120" Grid.Column="1"/>
        <Button Name="ButtonAbort" Content="Отключить" HorizontalAlignment="Right" Margin="0,0,10,10" VerticalAlignment="Bottom" Width="120" Click="ButtonAbort_Click" Grid.Column="1"/>
        <Label Content="Статус сервера: " Margin="16,19,0,0" VerticalAlignment="Top" HorizontalAlignment="Left" Width="101"/>
        <Label Name="LabelStatus" Content="Disconnect" Margin="134,19,0,0" VerticalAlignment="Top" HorizontalAlignment="Left" />
        <WindowsFormsHost Name="Host" Margin="10,69,10,48" Grid.ColumnSpan="2"/>
        <CheckBox Name="CheckBoxNeadToSaveTrade" Content="Сохранять историю" HorizontalAlignment="Left" Margin="90.333,15,0,0" VerticalAlignment="Top" Grid.Column="1"/>
        <TextBox x:Name="TextBoxCountDaysSave" HorizontalAlignment="Right" Height="23" Margin="0,41,10,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="74" Grid.Column="1"/>
        <Label Content="Дней подгружать: " Margin="90.333,39,0,0" VerticalAlignment="Top" HorizontalAlignment="Left" Width="118" Grid.Column="1"/>
    </Grid>
</Window>
