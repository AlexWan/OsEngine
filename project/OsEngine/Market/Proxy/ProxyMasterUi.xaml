<Window x:Class="OsEngine.Market.Proxy.ProxyMasterUi"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:OsEngine.Market.Proxy"
        mc:Ignorable="d"
        Title="Proxy" Height="306" Width="598" WindowStartupLocation="CenterScreen" Style="{StaticResource WindowStyleCanResize}" Icon="/Images/OsLogo.ico">
    <Grid>
        <TabControl Foreground="Black" Margin="0,0,0,0">
            <TabItem Name="TabItem1" Header=" Proxies ">
                <Grid>
                    <WindowsFormsHost x:Name="HostProxy" Foreground="Black" Margin="0,0,0,0"/>
                </Grid>
            </TabItem>
            <TabItem Name="TabItem2" Header=" Logging ">
                <Grid>
                    <WindowsFormsHost Name="HostLog" Foreground="Black" Margin="0,0,0,0"/>
                </Grid>
            </TabItem>
        </TabControl>

    </Grid>
</Window>
