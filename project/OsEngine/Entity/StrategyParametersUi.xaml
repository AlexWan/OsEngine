<Window x:Class="OsEngine.Entity.StrategyParametersUi"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
       xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Parameters" Height="450" MinHeight="200" MinWidth="530" Width="626" Topmost="False" Style="{StaticResource WindowStyleCanResize}" WindowStartupLocation="CenterOwner"  Icon="/Images/OsLogo.ico">
    <Grid>
        <Button Name="ButtonAccept" Content="Accept" Margin="0,0,5,5" VerticalAlignment="Bottom" HorizontalAlignment="Right" Width="125" Click="ButtonAccept_Click"/>
        <TabControl Name="TabControlSettings" Margin="0,0,0,30">
        </TabControl>
        <Button x:Name="ButtonUpdate" Content="Update" Margin="0,0,135,5" VerticalAlignment="Bottom" HorizontalAlignment="Right" Width="125" Click="ButtonUpdate_Click" />

        <Button x:Name="ButtonSaveSettings" Content="Save" Margin="5,0,0,5" VerticalAlignment="Bottom" HorizontalAlignment="Left" Width="125" Click="ButtonSaveSettings_Click" />
        <Button x:Name="ButtonLoadSettings" Content="Load" Margin="135,0,0,5" VerticalAlignment="Bottom" HorizontalAlignment="Left" Width="125" Click="ButtonLoadSettings_Click" />

    </Grid>
</Window>

